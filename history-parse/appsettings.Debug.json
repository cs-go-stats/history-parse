{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],

  "RabbitMqConnection": {
    "Host": "localhost",
    "Port": 5672,
    "Username": "guest",
    "Password": "guest",
    "Heartbeat": 1000
  },

  "MatchDateLowerBound": {
    "Year": 2015,
    "Month": 1,
    "Day": 1
  },

  "MatchStars": {
    "LowerBound": 1
  },

  "Logging": {
    "MessageTemplate": "{NewLine}{Timestamp:HH:mm:ss} [{Level}] ({CorrelationToken}) {Message}{NewLine}{Exception}",
    "Filename": "HistoryParse.log",
    "FileSizeLimit": 1048576
  },

  "PostgresConnection": {
    "Host": "127.0.0.1",
    "Database": "history-parse",
    "Username": "postgres",
    "Password": "dotFive1"
  }
}